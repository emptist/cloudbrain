# Cloud Brain GCP Configuration
# Copy this file to .env and fill in your values

# GCP Project Configuration
CLOUD_BRAIN_PROJECT_ID=your-project-id
CLOUD_BRAIN_REGION=us-central1

# Cloud SQL Instance Configuration
CLOUD_BRAIN_INSTANCE_NAME=cloudbrain-db
CLOUD_BRAIN_DB_NAME=cloudbrain
CLOUD_BRAIN_DB_USER=cloudbrain
CLOUD_BRAIN_DB_PASSWORD=your-secure-password-here

# Database Configuration
CLOUD_BRAIN_DB_TYPE=postgresql
CLOUD_BRAIN_DB_TIER=db-f1-micro
CLOUD_BRAIN_STORAGE_SIZE=10

# Connection String (auto-generated, do not edit)
# CLOUD_BRAIN_CONNECTION_STRING=host=/cloudsql/... dbname=... user=... password=...

# AI Configuration
CLOUD_BRAIN_AI_ID=1
CLOUD_BRAIN_AI_NAME=AI_Assistant

# Development Settings
CLOUD_BRAIN_ENVIRONMENT=development
CLOUD_BRAIN_DEBUG=false